#!/bin/sh /etc/rc.common

START=99
STOP=99

start() {
  if [ ! -f /Oya/pid/recording ]; then
    sh /Oya/recording.sh
  fi
}

stop() {
  recPid=`recording pid get`
  arecPid=`recording apid get`
  kill ${recPid}
  kill ${arecPid}
  recording remove pid
  recording remove apid
}

boot() {
	/etc/init.d/recording enable
}
