#!/bin/sh

case $1 in
send)
  echo -e $2 > /dev/ttyS2
  ;;
read)
  uartcommand=`cat /Oya/uart/uartcommand`
  echo ${uartcommand}
  ;;
pid)
 case $2 in
    set)
    pid=$4
    echo ${pid} > /Oya/pid/uart_process
    ;;
    get)
    pid=`cat /Oya/pid/uart_process`
    echo ${pid}
    ;;
    remove)
    rm /Oya/pid/uart_process
    ;;
    esac
  ;;
esac
